<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uni.projects.at.hangman">

    <application
        android:allowBackup="true"
        android:icon="@drawable/hangman_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/hangman_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ScoreActivity" />
        <activity android:name=".MailActivity" />
        <activity android:name=".SendWordActivity" />
        <activity android:name=".PlayersListActivity" />
        <activity android:name=".WelcomeActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".PlayActivity" />
        <activity android:name=".PlayMultyActivity" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>